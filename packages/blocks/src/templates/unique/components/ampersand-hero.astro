            decoding='async'
            loading='lazy'
            alt='prodkt'
            class='absolute -top-[1dvw] z-0 mx-auto my-auto h-auto w-[16dvw] select-none'
          />

          <svg
            class='levelnSvg absolute mx-auto my-auto h-auto w-32 -translate-y-[13dvw] translate-x-[27dvw] select-none sm:w-44 sm:-translate-y-[9dvw] md:w-56 md:-translate-y-[7.5dvw] xl:w-64'
            viewBox='0 0 204 58'
            fill='none'
            xmlns='http://www.w3.org/2000/svg'
          >

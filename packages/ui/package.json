{
  "name": "@prodkt/ui",
  "version": "0.0.1",
  "private": true,
  "description": "Prodkt UI Design System Components Library",
  "license": "MIT",
  "author": "Bryan Funk",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./primitives/*": {
      "types": "./dist/primitives/*/index.d.ts",
      "require": "./dist/primitives/*/index.cjs",
      "default": "./dist/primitives/*/index.js"
    },
    "./brands/*": {
      "types": "./dist/brands/*/index.d.ts",
      "require": "./dist/brands/*/index.cjs",
      "default": "./dist/brands/*/index.js"
    },
    "./prodkt-components/*": {
      "types": "./dist/prodkt-components/*/index.d.ts",
      "require": "./dist/prodkt-components/*/index.cjs",
      "default": "./dist/prodkt-components/*/index.js"
    },
    "./icons/*": {
      "types": "./dist/icons/*/index.d.ts",
      "require": "./dist/icons/*/index.cjs",
      "default": "./dist/icons/*/index.js"
    },
    "./cn/*": {
      "types": "./dist/utils/cn.d.ts",
      "require": "./dist/utils/cn.cjs",
      "default": "./dist/utils/cn.js"
    },
    "./useMounted/*": {
      "types": "./dist/utils/use-mounted.d.ts",
      "require": "./dist/utils/use-mounted.cjs",
      "default": "./dist/utils/use-mounted.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build-storybook": "storybook build",
    "clean": "bun run rm -rf dist storybook-static *.tsbuildinfo",
    "dev": "vite build -w",
    "lint": "eslint .  --max-warnings 0",
    "storybook": "storybook dev -p 6006 --no-open",
    "test-storybook": "test-storybook",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.19.8",
